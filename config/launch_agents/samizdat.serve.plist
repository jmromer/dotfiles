<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>Disabled</key>
	<true/>
	<key>KeepAlive</key>
	<true/>
	<key>Label</key>
	<string>samizdat.serve</string>
	<key>Nice</key>
	<integer>1</integer>
	<key>Program</key>
	<string>/bin/zsh</string>
	<key>ProgramArguments</key>
	<array>
		<string>/bin/zsh</string>
		<string>-c</string>
		<string>${DOTFILES_DIR}/bin/samizdat serve</string>
	</array>
	<key>RunAtLoad</key>
	<true/>
	<key>StandardErrorPath</key>
	<string>/tmp/samizdat.serve.err</string>
	<key>StandardOutPath</key>
	<string>/tmp/samizdat.serve.out</string>
</dict>
</plist>
